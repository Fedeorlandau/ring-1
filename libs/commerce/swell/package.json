{
  "name": "@ring/swell",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "sideEffects": false,
  "scripts": {
    "build-lib": "yarn _tsup --minify",
    "dev": "yarn _tsup --watch",
    "_tsup": "tsup src/index.ts --format esm,cjs --dts --external react",
    "lint": "run-p _lint:*",
    "_lint:eslint": "TIMING=1 eslint src --format stylish --max-warnings=0",
    "_lint:tsc": "tsc --noEmit",
    "test": "jest",
    "test-coverage": "jest --coverage",
    "clean": "run-p _clean:*",
    "_clean:dist": "shx rm -rf ./dist",
    "_clean:node_modules": "shx rm -rf ./node_modules",
    "_clean:turbo": "shx rm -rf ./.turbo"
  },
  "dependencies": {
    "@ring/contracts": "*",
    "@ring/logger": "*",
    "@ring/ui": "*",
    "node-fetch": "2.6.0"
  },
  "devDependencies": {
    "@ring/config-eslint": "*",
    "@ring/config-jest": "*",
    "@ring/config-release": "*",
    "@ring/config-typescript": "*",
    "@swc/core": "1.2.242",
    "@swc/jest": "0.2.22",
    "@types/jest": "29.0.0",
    "@types/node": "18.7.13",
    "eslint": "8.22.0",
    "jest": "29.0.1",
    "jest-environment-jsdom": "28.1.3",
    "msw": "0.45.0",
    "npm-run-all": "4.1.5",
    "tsup": "6.2.2",
    "typescript": "4.7.4"
  }
}
