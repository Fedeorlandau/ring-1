{
  "name": "@ring/blog",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "prepare": "shx cp -u .env.example .env",
    "dev": "cross-env NODE_OPTIONS='--inspect' next dev",
    "lint": "run-p _lint:*",
    "_lint:eslint": "cross-env TIMING=1 eslint src --format stylish --max-warnings=0",
    "_lint:tsc": "tsc --noEmit",
    "test": "jest",
    "test-coverage": "jest --coverage",
    "build": "next build",
    "start": "next start",
    "clean": "run-p _clean:*",
    "_clean:next": "shx rm -rf ./.next",
    "_clean:node_modules": "shx rm -rf ./node_modules",
    "_clean:turbo": "shx rm -rf ./.turbo"
  },
  "msw": {
    "workerDirectory": "public"
  },
  "dependencies": {
    "@ring/logger": "*",
    "@ring/ui": "*",
    "@types/lodash.startcase": "4.4.7",
    "clsx": "1.2.1",
    "lodash.startcase": "4.4.0",
    "next": "12.2.5",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@ring/config-eslint": "*",
    "@ring/config-jest": "*",
    "@ring/config-release": "*",
    "@ring/config-typescript": "*",
    "@swc/core": "1.2.241",
    "@swc/jest": "0.2.22",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.3.0",
    "@testing-library/user-event": "14.4.3",
    "@types/debug": "4.1.7",
    "@types/jest": "28.1.7",
    "@types/node": "18.7.13",
    "@types/react": "18.0.17",
    "@types/react-datepicker": "4.4.2",
    "@types/react-dom": "18.0.6",
    "autoprefixer": "10.4.8",
    "cross-env": "7.0.3",
    "eslint": "8.22.0",
    "gray-matter": "4.0.3",
    "jest": "28.1.3",
    "jest-environment-jsdom": "28.1.3",
    "jest-next-dynamic": "1.0.1",
    "msw": "0.45.0",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.16",
    "prettier-plugin-tailwindcss": "0.1.13",
    "semantic-release": "19.0.4",
    "semantic-release-monorepo": "7.0.5",
    "shx": "0.3.4",
    "tailwindcss": "3.1.8",
    "typescript": "4.7.4"
  }
}
